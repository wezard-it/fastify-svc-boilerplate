{
  "name": "wezard",
  "version": "0.0.1",
  "description": "Wezard service",
  "main": "index.js",
  "scripts": {
    "dev": "dotenv ts-node ./index.ts",
    "clean": "rm -rf node_modules/",
    "build": "tsc -p .",
    "watch": "dotenv ts-node-dev -- --files ./index.ts",
    "lint": "eslint --ext .ts \"src/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "start": "dotenv node ./dist/index.js",
    "typecheck": "tsc --extendedDiagnostics --noEmit --project .",
    "commit": "npx git-cz",
    "prepare": "husky install",
    "test": "jest -i",
    "plugin:generate": "node wezard-scripts/plugin-generator/index.js",
    "schema:generate": "node wezard-scripts/schema-generator/index.js",
    "prisma:generate": "prisma generate",
    "database:seed": "node wezard-scripts/seed-database/index.js"
  },
  "keywords": [
    "wezard-svc"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Paolo Destefanis <paolo.destefanis@wezard.it>",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@jest/globals": "^29.7.0",
    "@types/busboy": "^1.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.7",
    "prisma": "^5.4.2",
    "prisma-dbml-generator": "^0.11.0-dev.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@fastify/cors": "^8.2.1",
    "@fastify/helmet": "^10.1.0",
    "@fastify/multipart": "^7.6.0",
    "@fastify/request-context": "^4.2.0",
    "@google-cloud/storage": "^7.7.0",
    "@logtail/pino": "^0.4.0",
    "@logtail/winston": "^0.4.1",
    "@prisma/client": "^5.4.2",
    "@sendgrid/mail": "^7.7.0",
    "ajv": "^8.12.0",
    "axios": "^1.4.0",
    "bluebird": "^3.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "fast-json-stringify": "^5.7.0",
    "fastify": "^4.15.0",
    "fastify-cron": "^1.3.1",
    "firebase-admin": "^11.6.0",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "node-mailjet": "^6.0.5",
    "typescript-json-schema": "^0.62.0",
    "winston": "^3.8.2",
    "winston-transport": "^4.7.0",
    "winston-transport-sentry-node": "^2.7.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/config-conventional"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
