{
  "name": "wezard",
  "version": "0.0.1",
  "description": "Wezard service",
  "main": "index.js",
  "scripts": {
    "dev": "dotenv ts-node ./index.ts",
    "clean": "rm -rf node_modules/",
    "build": "tsc -p .",
    "watch": "dotenv ts-node-dev -- --files ./index.ts",
    "lint": "eslint --ext .ts \"src/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "start": "dotenv node ./dist/index.js",
    "typecheck": "tsc --extendedDiagnostics --noEmit --project .",
    "commit": "npx git-cz",
    "prepare": "husky install",
    "test": "jest -i",
    "plugin:generate": "node wezard-scripts/plugin-generator/index.js",
    "schema:generate": "node wezard-scripts/schema-generator/index.js",
    "prisma:generate": "prisma generate",
    "database:seed": "node wezard-scripts/seed-database/index.js"
  },
  "keywords": [
    "wezard-svc"
  ],
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=1.22.22"
  },
  "author": "Paolo Destefanis <paolo.destefanis@wezard.it>",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@jest/globals": "^30.1.2",
    "@types/busboy": "^1.5.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "git-cz": "^4.9.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "pre-commit": "^1.2.2",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "prisma-dbml-generator": "^0.12.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.2.1",
    "@fastify/request-context": "^6.2.1",
    "@google-cloud/storage": "^7.17.0",
    "@logtail/pino": "^0.5.6",
    "@logtail/winston": "^0.5.6",
    "@prisma/client": "^6.15.0",
    "@sendgrid/mail": "^8.1.5",
    "ajv": "^8.17.1",
    "axios": "^1.11.0",
    "bluebird": "^3.7.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "dotenv-cli": "^10.0.0",
    "fast-json-stringify": "^6.0.1",
    "fastify": "^5.6.0",
    "fastify-cron": "^1.4.0",
    "firebase-admin": "^13.5.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.0",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.6.0",
    "node-mailjet": "^6.0.9",
    "typescript-json-schema": "^0.65.1",
    "winston": "^3.17.0",
    "winston-transport": "^4.9.0",
    "winston-transport-sentry-node": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/config-conventional"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
